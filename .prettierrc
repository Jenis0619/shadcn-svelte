{
  "useTabs": false,
  "singleQuote": false,
  "trailingComma": "none",
  "printWidth": 80,
  "plugins": [
    "prettier-plugin-svelte"
  ],
  "pluginSearchDirs": false,
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte",
        "svelteIndentScriptAndStyle": true,
        "svelteStrictMode": false,
        "svelteSortOrder": "scripts-markup-styles-options"
      }
    },
    {
      "files": "*.md",
      "options": {
        "useTabs": false,
        "tabWidth": 2
      }
    }
  ],
  "bracketSameLine": false
}